<div id="sidebarcontainer">
<%parsedinclude(sidebar.inc)%>
</div>

<!-- page footer -->
<%parsedinclude(footer.inc)%>]]></part>

		<part name="archivelist"><![CDATA[<%parsedinclude(head.inc)%>

<!-- page header -->
<%parsedinclude(header.inc)%>

<!-- page content -->
